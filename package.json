    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "gh-pages": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "prisma": "^5.7.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
